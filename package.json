{
  "name": "vue-sub",
  "version": "0.0.8",
  "description": "Simple observer pattern implementation for VueJS",
  "main": "lib/vue-sub.js",
  "types": "types/index.d.ts",
  "files": [
    "src/*.js",
    "lib/*.js",
    "types/*.d.ts"
  ],
  "maintainers": [
    {
      "name": "eazymov",
      "email": "eazymovcode@gmail.com"
    }
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "npm run build:prod && npm run build:test",
    "build:prod": "tsc && webpack --config config/prod.config.js",
    "build:test": "webpack --config config/test.config.js",
    "test": "npm run test:types && npm run test:unit && npm run test:e2e",
    "test:types": "tsc -p test/types/tsconfig.json",
    "test:unit": "babel-node test/unit/runner.js",
    "test:e2e": "node test/e2e/runner.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eazymov/vue-sub.git"
  },
  "keywords": [
    "vue",
    "sub",
    "observer",
    "observable",
    "plugin",
    "javascript",
    "decorators",
    "typescript"
  ],
  "author": {
    "name": "Eazymov",
    "email": "eazymovcode@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Eazymov/vue-sub/issues"
  },
  "homepage": "https://github.com/Eazymov/vue-sub#readme",
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-env": "^1.6.1",
    "chromedriver": "^2.33.2",
    "cross-spawn": "^5.1.0",
    "http-server": "^0.10.0",
    "jasmine": "^2.8.0",
    "nightwatch": "^0.9.16",
    "selenium-server": "^3.6.0",
    "ts-loader": "^3.0.3",
    "typescript": "^2.5.3",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "vue": "^2.5.3",
    "vue-class-component": "^6.1.0"
  }
}
